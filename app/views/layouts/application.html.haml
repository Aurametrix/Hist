!!! 5

%html
  %head
    %title AAA: 
    = stylesheet_link_tag 'compiled/grid.css', :media => 'screen, projection'
    = stylesheet_link_tag 'south-street/jquery-ui.css', :media => 'screen, projection'
    
    = javascript_include_tag :defaults
    = csrf_meta_tag
    
  %body
    #header
      .container
        %hgroup
          = link_to root_path do
            = image_tag "logo.png"
            %h1 AAA
          .clear
    
    .container
      #notices
        .notice
          = notice
        .alert
          = alert
        
    .container      
      = yield
